                <TextView
                    android:id="@+id/Tv_tag_mjj"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignEnd="@+id/img_mjj"
                    android:layout_alignRight="@+id/textView47"
                    android:layout_alignTop="@+id/img_mjj"
                    
