                    android:textSize="15sp"/>

                <TextView
                    android:id="@+id/Tv_tag_gp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/img_gp"
                    android:textColor="@color/my_white"
                    android:textSize="@dimen/small"/>

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/Rl_gd"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.25"
                android:clickable="true">

                <ImageView
                    android:id="@+id/img_bj"
                    android:layout_width="56dp"
                    android:layout_height="56dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="10dp"
                    android:src="@drawable/index_c_bj" android:clickable="true"/>

                <TextView
                    android:id="@+id/textView51"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/img_bj"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="3dp"
                    android:text="@string/index_bj"
                    android:textColor="@color/my_black1"
                    android:textSize="15sp"/>

                <TextView
                    android:id="@+id/Tv_tag_gd"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignTop="@+id/img_bj"
                    android:textColor="@color/my_white"
                    android:textSize="@dimen/small"/>

