<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
    android:background="#E9E9E9">
      <RelativeLayout
           xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="0dp" android:layout_weight="2.07"
          android:background="@drawable/mjj_hl_icon"
          android:id="@+id/Rlbl"></RelativeLayout>
      <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

            <ImageView
            android:layout_width="339dp"
            android:layout_height="148dp"
            android:id="@+id/imageView29"
            android:src="@drawable/mjj_btn_bg"
            android:layout_centerHorizontal="true" android:layout_margin="8dp"
            android:scaleType="fitXY"/>
        <ImageView
            android:layout_width="113dp"
            android:layout_height="74dp"
            android:id="@+id/img_tz"
            android:src="@drawable/ims_btn_tz"
            android:clickable="true"
            android:layout_alignTop="@+id/imageView29" android:layout_alignLeft="@+id/img_gb"
            android:layout_alignStart="@+id/img_gb"/>
        <ImageView android:layout_width="113dp" android:layout_height="74dp"
                   android:id="@+id/img_zk"
                   android:src="@drawable/ims_btn_zk"
                   android:nestedScrollingEnabled="false" android:clickable="true"
                   android:layout_alignTop="@+id/imageView29" android:layout_toLeftOf="@+id/img_tz"
                   android:layout_toStartOf="@+id/img_tz"/>
        <ImageView android:layout_width="113dp" android:layout_height="74dp"
                   android:id="@+id/img_yk"
                   android:src="@drawable/ims_btn_yk"
                   android:clickable="true"
                   android:layout_alignTop="@+id/imageView29" android:layout_toRightOf="@+id/img_tz"
                   android:layout_toEndOf="@+id/img_tz"/>
        <ImageView android:layout_width="113dp" android:layout_height="74dp"
                   android:id="@+id/img_gb"
                   android:src="@drawable/ims_btn_dy"
                   android:clickable="true"
                   android:layout_below="@+id/img_tz" android:layout_centerHorizontal="true"/>
        <ImageView android:layout_width="113dp" android:layout_height="74dp"
                   android:id="@+id/img_tf" android:src="@drawable/ims_btn_tf"
                   android:clickable="true"
                   android:layout_below="@+id/img_zk" android:layout_alignLeft="@+id/imageView29"
                   android:layout_alignStart="@+id/imageView29"/>
        <ImageView android:layout_width="113dp" android:layout_height="74dp"
                   android:id="@+id/img_hl" android:src="@drawable/ims_btn_hl"
                   android:clickable="true"
                   android:layout_below="@+id/img_yk" android:layout_toRightOf="@+id/img_gb"
                   android:layout_toEndOf="@+id/img_gb"/>

    </RelativeLayout>
</LinearLayout>
