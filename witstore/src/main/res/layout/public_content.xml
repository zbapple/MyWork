<?xml version="1.0" encoding="utf-8"?>


    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="132dp"
        android:background="@drawable/light_c_wz_bg" android:id="@+id/rl_head">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_two"
        android:textSize="@dimen/abc_text_size_title_material"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_three"
        android:textSize="@dimen/abc_text_size_title_material"
        android:layout_alignTop="@+id/tv_two"
        android:layout_toRightOf="@+id/tv_two"
        android:layout_toEndOf="@+id/tv_two"
        android:layout_marginLeft="11dp"
        android:layout_marginStart="11dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_one"
        android:textSize="@dimen/abc_text_size_title_material"
        android:layout_alignTop="@+id/tv_two"
        android:layout_toLeftOf="@+id/tv_two"
        android:layout_toStartOf="@+id/tv_two"
        android:layout_marginRight="13dp"
        android:layout_marginEnd="13dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView"
        android:src="@drawable/door_c_wz_icon"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</RelativeLayout>
